//ğŸ€ğŸ€ğŸ€ğŸ€ğŸ€ğŸ€ğŸ€ğŸ€ğŸ€
int dp[4001];
int a,b,c,n;
int cal(int sum){
   
    if(sum==0)return 0;
    if(sum<0)return -1e9; 
    if(dp[sum])return dp[sum];
    return dp[sum]=max(cal(sum-a)+1,max(cal(sum-b)+1,cal(sum-c)+1));



}

int main(){
    ios_base::sync_with_stdio(0); cin.tie(NULL);
    cin>>n>>a>>b>>c;
    cout<<cal(n);
   return 0;
}
