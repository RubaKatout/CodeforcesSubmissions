#include<bits/stdc++.h>
typedef long long ll;
using namespace std;
 ll oo=1e18;


int main(){
    int hi=1e6,lo=1,md,ans;
    while (lo<=hi){
        md=(hi+lo)/2;
        cout<<md<<endl;

        string s;
        cin>>s;

        if(s=="<")hi=md-1;
        else ans=md,lo=md+1;
        }
       cout<<"! "<<ans<<endl;



    return 0;
}
