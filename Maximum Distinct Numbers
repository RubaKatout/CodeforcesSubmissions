#include <bits/stdc++.h>
using namespace std ;

int main() {

     long long n ;
     cin >> n ;

     long long l = 1 , r = 2e9 , md , ans;
     while(l <= r){
         md = (l + r) / 2 ;
         if(md * (md + 1) / 2 <= n)ans = md , l = md + 1 ;
         else r = md - 1 ;


     }
     cout << ans ;


}
